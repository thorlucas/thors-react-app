{
	"extends": "@tsconfig/vite-react/tsconfig.json",
	"compilerOptions": {
		"useDefineForClassFields": true,
		"types": [
			"vite/client"
		],
		"paths": {
			"@components/": ["./src/components/*"],
			"@assets/": ["./src/assets/*"],
			"@pages/*": ["./src/pages/*"],
			"@/": ["./src/"]
		}
	},
	"references": [
		{
			"path": "./tsconfig.node.json"
		}
	],
	"include": [
		"src"
	]
}
