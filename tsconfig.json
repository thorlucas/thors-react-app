{
	"compilerOptions": {
		"outDir": "./dist/",
		"module": "esnext",
		"target": "es5",
		"jsx": "react-jsx",
		"moduleResolution": "node",

		"allowJs": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"sourceMap": true,
	}
}
